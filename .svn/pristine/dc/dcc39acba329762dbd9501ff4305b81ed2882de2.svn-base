  <select id="selectPurchaseLst" resultType="map" parameterType="map" >
    SELECT a.*,b.id pid,b.`name` pname,b.`code` pcode,b.createtime pcreatetime,b.num pnum FROM `jnd_need` a LEFT JOIN jnd_buy b ON a.id=b.needid WHERE a.`status` IS NULL and a.num>0
    <if test="name != null" >
      and b.`name` like concat('%',#{name},'%')
    </if>
    <if test="code != null" >
      and b.`code` like concat('%',#{code},'%')
    </if>
    <if test="time != null" >
      and DATE_FORMAT(b.`createtime`,'%Y-%m-%d') = #{time}
    </if>
    <if test="country != null" >
      and a.country =#{country}
    </if>
    <if test="forapay != null" >
      and b.id IS NOT NULL
    </if>
    order by a.createtime desc limit #{start},#{pageSize}
  </select>
