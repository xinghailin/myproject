    <select id="selectByMap" resultType="map" parameterType="map" >
SELECT
	a.*, b.clientype
FROM
	biz_contract a
JOIN biz_clientinfo b ON a.infoid = b.id where 1=1 
	<if test="node != null and node !=''" >
	<if test='node == "1"' >
      and (a.`status` is null or a.`status` in(1,2))
    </if>
    <if test='node == "2"' ><!--61金融门店确认审批  -->
      and a.`status` in(1,3,61)
    </if>
    <if test='node == "3"' >
      and a.finance=1 and a.`status` in(4,51,52,53)
    </if>
    <if test='node == "4"' >
      and a.finance=1 and a.`status` in(51,54,55,56)
    </if>
    <if test='node == "5"' >
      and a.finance=2 and a.`status` in(3,30)
    </if>
    </if>
    <if test="filecode != null and filecode !=''" >
       and a.filecode like concat('%',#{filecode},'%')
    </if>
    <if test="name != null and name !=''" >
       and a.name like concat('%',#{name},'%')
    </if>
    <if test="bname != null and bname !=''" >
       and a.bname like concat('%',#{bname},'%')
    </if>
    <if test="identify != null and identify !=''" >
       and a.identify like concat('%',#{identify},'%')
    </if>
    <if test="clientype != null and clientype !=''" >
       and b.clientype = #{clientype}
    </if>
    <if test="sellid != null and sellid !=''" >
       and a.sellid = #{sellid}
    </if>
    <if test="shopid != null and shopid !=''" >
       and a.shopid = #{shopid}
    </if>
    <if test="status==60" >
       and a.status=8 and a.status2 in(1,3,4,5)
    </if>
     <if test="status == 70" ><!-- 金融合同添加列表查询的数据  -->
       and a.status=62 and a.status2=1
    </if>
    <if test="status == 80" ><!-- 保险报价显示的信息 -->
       and a.status in(30,7,8)
    </if> 
     <if test="status == 90" ><!-- 金融合同选车 -->
       and a.status in(3,62) and a.finance=1 and a.status2 is null
    </if> 
    order by a.createtime desc
    limit #{start},#{size}
  </select>
