package com.yyq.car.portal.common.dto;

public class IntroduceDto {
	private String id_chinese;

	private String id_english;

	private String id_spaish;

	private String content_chinese;

	private String content_english;

	private String content_spaish;

	public String getId_chinese() {
		return id_chinese;
	}

	public void setId_chinese(String id_chinese) {
		this.id_chinese = id_chinese;
	}

	public String getId_english() {
		return id_english;
	}

	public void setId_english(String id_english) {
		this.id_english = id_english;
	}

	public String getId_spaish() {
		return id_spaish;
	}

	public void setId_spaish(String id_spaish) {
		this.id_spaish = id_spaish;
	}

	public String getContent_chinese() {
		return content_chinese;
	}

	public void setContent_chinese(String content_chinese) {
		this.content_chinese = content_chinese;
	}

	public String getContent_english() {
		return content_english;
	}

	public void setContent_english(String content_english) {
		this.content_english = content_english;
	}

	public String getContent_spaish() {
		return content_spaish;
	}

	public void setContent_spaish(String content_spaish) {
		this.content_spaish = content_spaish;
	}

}